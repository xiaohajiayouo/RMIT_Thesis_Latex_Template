  % \cleardoublepage
}

\newenvironment{dedication}{
  \let\cleardoublepage\clearpage
  \clearpage
  \begin{center}{
    \large \bf Dedication}
  \end{center}
  \small
  \linespread{1.3}
  \normalsize  
}{}
\def\enddedication{
  \par 
}
\newenvironment{acknowledgements}{
  % \cleardoublepage
  \let\cleardoublepage\clearpage
  \clearpage
  \begin{center}{
    \large \bf Acknowledgements}
    \setlength{\parindent}{2em}
  \end{center}
  \small
  \linespread{1.3}
  \normalsize
  \let\cleardoublepage\clearpage
}{}
\def\endacknowledgements{
  \par
}

\def\abstract{
